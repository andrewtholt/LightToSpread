^set GET_FORMAT $K
^set USER source
^set ADD_CR true
^set SET_FORMAT $K $V
^set CLIENT raw
^set DEFAULT_GROUP global
^set DEBUG false
^set SAVE_ALLOWED true
^set SPREAD_SERVER 4803
^set INTERACTIVE false
^set TO_FORTH false
^set AUTOCONNECT false
^set EXIT_ALLOWED true
^set APP_DIR /usr/local/etc/lightToSpread/App
^set MODE local
